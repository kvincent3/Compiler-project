///////////test1////////
do
  integer i,j
  function int fMax (x:integer , y:integer)
   integer result
   if x+--1 == y then 
         result =x
    else result=y
    fi
   return (result)
  end

 procedure theEnd ()
  write "thats'all"
 end

 i=-3
 j=0
 maximum = fMax(i,j)
 write maximum
 theEnd()
end

